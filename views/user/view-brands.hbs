<div class="mt-4 mb-4">
  <h3 class="ms-3 ms-md-5"> Brands </h3>
</div>

<div class="container vh-100">
  <div class="row">
    {{#each brands}}
    <div class="col-md-3 " style="cursor: pointer;">
      <div class=" p-3 mb-4 rounded shadow-lg brand-container">
        <h5 style="color: #F46F36;" class="text-center brand-name ">{{this}}</h5>
      </div>
    </div>
    {{/each}}
  </div>
</div>
